<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xuecheng.teaching.mapper.CourseWorkMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xuecheng.teaching.entity.CourseWork">
        <id column="id" property="id" />
        <result column="course_pub_id" property="coursePubId" />
        <result column="course_name" property="courseName" />
        <result column="company_id" property="companyId" />
        <result column="company_name" property="companyName" />

        <result column="work_number" property="workNumber" />
        <result column="answer_number" property="answerNumber" />
        <result column="tobe_reviewed" property="tobeReviewed" />
        <result column="commited_time" property="commitedTime" />
        <result column="reviewed_time" property="reviewedTime" />
        <result column="create_date" property="createDate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, course_pub_id, course_name, company_id, company_name, work_number, answer_number, tobe_reviewed, commited_time, reviewed_time, create_date
    </sql>

</mapper>
